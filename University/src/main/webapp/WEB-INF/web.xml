<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Form1</servlet-name>
        <servlet-class>com.mycompany.university.Form1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Form2</servlet-name>
        <servlet-class>com.mycompany.university.Form2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Form3</servlet-name>
        <servlet-class>com.mycompany.university.Form3</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registration</servlet-name>
        <servlet-class>com.mycompany.university.Registration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>View</servlet-name>
        <servlet-class>com.mycompany.university.View</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Form1</servlet-name>
        <url-pattern>/Form1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Form2</servlet-name>
        <url-pattern>/Form2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Form3</servlet-name>
        <url-pattern>/Form3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/Registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>View</servlet-name>
        <url-pattern>/View</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
